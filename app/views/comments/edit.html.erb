<div class="webcontainer">
<h1>Edit this Comment!</h1>

<%= form_for @comment do |f| %>

    <div class="field">
    <%= f.label :content %>
    <%= f.text_field :content %>
    </div>

    <%= f.submit "Submit these changes!" %>

<% end %>

<%= link_to "Back to Today's Matches!", matches_path %>
</div>
