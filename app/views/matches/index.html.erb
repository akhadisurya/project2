<h1>Today's Matches: Please fulfill my fantasy <%= emojify(":smiling_imp:") %></h1>


<ul>
    <% @matches.each do |match| %>
        <li>
            <strong> <%= match.first_name %> </strong>
            <div class="field">

  </div>

            <img src="<%= match.image %>" width=200px height=200px>
            <%= link_to emojify("Check Me Out :heart:"), match_path(match) %>

        </li>
    <% end %>
</ul>


