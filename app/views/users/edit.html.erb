<div class="webcontainer">

<div class="edituser">
<h1>Edit</h1>

<%= form_for @user do |f| %>

    <div class="field">
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
    </div>

     <div class="field">
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
     </div>

    <div class="field">
    <%= f.label :gender %><br>
    <%= f.text_field :gender %>
    </div>

    <div class="field">
    <%= f.label :preference %><br>
    <%= f.text_field :preference %>
    </div>

    <div class="field">
    <%= f.label :age %><br>
    <%= f.text_field :age %>
    </div>
  <br>
    <%= f.submit "Submit these changes!" %>

<% end %>
<br>
<%= link_to "Back to Today's Matches!", matches_path %>
</div>
</div>
