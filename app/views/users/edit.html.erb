<div class="webcontainer">

<h1>Edit this User!</h1>

<%= form_for @user do |f| %>

    <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    </div>

  <div class="field">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>

  <div class="field">
    <%= f.label :age %>
    <%= f.text_field :age %>
  </div>

    <%= f.submit "Submit these changes!" %>

<% end %>

<%= link_to "Back to Today's Matches!", matches_path %>
</div>
